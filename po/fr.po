msgid_plural "%lu minutes"
msgstr[0] "%lu minute"
msgstr[1] "%lu minutes"

#: ../src/client/Client.vala:106
msgid "This computer will lock after %s %s. %s"
msgstr "Cet ordinateur se verrouillera dans %s %s. %s"

#: ../src/client/Client.vala:108
msgid "Time left"
msgstr "Temps restant"
